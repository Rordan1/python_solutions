import math
import os
import random
import re
import sys

def WeirdNumber():
    n = int(input().strip())
        
    if n % 2 == 1:
        print ("Weird")
        
    elif n % 2 != 1 and 2 <= n <= 5:
        print ("Not Weird")
        
    elif n % 2 != 1 and 6 <= n <= 20:
        print ("Weird")
        
    elif n % 2 != 1 and 20 < n:
        print ("Not Weird")

WeirdNumber()
